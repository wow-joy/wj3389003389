{
  "private": true,
  "workspaces": [
    "packages/*",
    "example/*"
  ],
  "description": "Wowjoy front",
  "scripts": {
    "install": "lerna bootstrap",
    "build": "npm run clean && father build",
    "build:watch": "father build --watch",
    "prettier": "prettier --write **/*.{js,jsx,tsx,ts,json} !**/es/** !**/lib/**",
    "changelog": "lerna-changelog",
    "test": "jest",
    "clean": "rm -rf packages/**/es packages/**/lib .docz .doc",
    "doc:dev": "dumi dev",
    "doc:build": "dumi build",
    "now-build": "npm run doc:build",
    "publish": "lerna publish from-package"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json} !**/es/** !**/lib/**": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "gitHber",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/runtime": "^7.7.6",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "dumi": "^1.0.31",
    "father": "^2.28.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lerna": "^3.18.1",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-test-renderer": "^16.12.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@hookform/error-message": "^0.0.4",
    "@material-ui/core": "^4.11.0",
    "antd": "^4.5.4",
    "lodash": "^4.17.20",
    "react-hook-form": "^6.4.1"
  }
}
